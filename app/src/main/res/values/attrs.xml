<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ViewPagerIndicator">
        <attr name="littleDotRes" format="reference" />
        <attr name="littleDotResSelect" format="reference" />
        <attr name="dividerRes" format="reference" />
        <attr name="dividerLength" format="dimension" />
    </declare-styleable>
    <declare-styleable name="RectImageView">
        <attr name="boundWidth" format="dimension"/>
        <attr name="boundColor" format="color"/>
        <attr name="boundFillColor" format="color"/>
        <attr name="showBound" format="boolean"/>
        <attr name="showMask" format="boolean"/>
        <attr name="maskColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="ToolSeekBar">
        <attr name="progress_color" format="color"/>
        <attr name="no_progress_color"/>
    </declare-styleable>
    <attr name="no_progress_color" format="color" />
    <declare-styleable name="NavigationTabStrip">
        <attr name="nts_color" format="color"/>
        <attr name="nts_size" format="dimension"/>
        <attr name="nts_bold" format="boolean"/>
        <attr name="nts_weight" format="dimension"/>
        <attr name="nts_factor" format="float"/>
        <attr name="nts_type">
            <enum name="none" value="0" />
            <enum name="line" value="1" />
            <enum name="point" value="2" />
        </attr>
        <attr name="nts_gravity">
            <enum name="background" value="2" />
            <enum name="bottom" value="0" />
            <enum name="top" value="1" />
        </attr>
        <attr name="nts_typeface" format="string" />
        <attr name="nts_inactive_color" format="color" />
        <attr name="no_progress_color"/>
        <attr name="nts_active_color" format="color" />
        <attr name="nts_animation_duration" format="integer" />
        <attr name="nts_corners_radius" format="dimension" />
        <attr name="nts_titles" format="reference" />

    </declare-styleable>
</resources>